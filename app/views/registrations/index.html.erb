<p id="notice"><%= notice %></p>

<h1>Listing Registrations</h1>

<table>
  <thead>
    <tr>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Phone</th>
      <th>Donation</th>
      <th>Participant</th>
      <th>Xs shirts</th>
      <th>S shirts</th>
      <th>M shirts</th>
      <th>L shirts</th>
      <th>Xl shirts</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registrations.each do |registration| %>
      <tr>
        <td><%= registration.street %></td>
        <td><%= registration.city %></td>
        <td><%= registration.state %></td>
        <td><%= registration.zip %></td>
        <td><%= registration.phone %></td>
        <td><%= registration.donation %></td>
        <td><%= registration.participants[0] %></td>
        <td><%= registration.xs_shirts %></td>
        <td><%= registration.s_shirts %></td>
        <td><%= registration.m_shirts %></td>
        <td><%= registration.l_shirts %></td>
        <td><%= registration.xl_shirts %></td>
        <td><%= link_to 'Show', registration %></td>
        <td><%= link_to 'Edit', edit_registration_path(registration) %></td>
        <td><%= link_to 'Destroy', registration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registration', new_registration_path %>
